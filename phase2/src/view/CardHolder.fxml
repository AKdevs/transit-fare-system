<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="420.0" prefWidth="541.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="CardHolderController">
  <children>
    <ChoiceBox id="choicebox" fx:id="cards" layoutX="196.0" layoutY="126.0" prefWidth="150.0" />
    <Label layoutX="14.0" prefHeight="46.0" prefWidth="150.0" text="Card Holder" textAlignment="CENTER" textOverrun="CENTER_ELLIPSIS">
      <font>
        <Font size="24.0" />
      </font>
    </Label>
      <VBox layoutX="14.0" layoutY="36.0" prefHeight="62.0" prefWidth="121.0">
         <children>
            <Label text="Account Number: ">
               <font>
                  <Font size="14.0" />
               </font>
            </Label>
            <Label text="Name:">
               <font>
                  <Font size="14.0" />
               </font>
            </Label>
            <Label text="Email: ">
               <font>
                  <Font size="14.0" />
               </font>
            </Label>
         </children>
      </VBox>
    <Label layoutX="25.0" layoutY="120.0" prefHeight="39.0" prefWidth="246.0" text="Travel Cards:">
      <font>
        <Font name="Century Gothic" size="18.0" />
      </font>
    </Label>
      <VBox layoutX="146.0" layoutY="40.0" prefHeight="200.0" prefWidth="100.0">
         <children>
            <Label fx:id="cardNumber" />
            <Label fx:id="name" />
            <Label fx:id="email" />
         </children>
      </VBox>
      <VBox layoutX="54.0" layoutY="183.0" prefHeight="200.0" prefWidth="150.0">
         <children>
            <Button fx:id="viewMonthlyCost" mnemonicParsing="false" onAction="#viewMonthlyCostButtonPushed" prefHeight="27.0" prefWidth="141.0" text="View Monthly Cost" />
            <Label text="Your monthly cost is:" />
            <Label fx:id="monthlyCost" />
         </children>
      </VBox>
      <Button fx:id="logOut" layoutX="430.0" layoutY="364.0" mnemonicParsing="false" onAction="#logOutButtonPushed" prefHeight="46.0" prefWidth="97.0" text="Log Out">
         <font>
            <Font size="14.0" />
         </font>
      </Button>
      <Button fx:id="goToCard" layoutX="401.0" layoutY="126.0" mnemonicParsing="false" onAction="#goToCardButtonPushed" text="Go To Card" />
  </children>
</AnchorPane>
