<VBox prefHeight="375.0" prefWidth="541.0" spacing="10.0" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="PasswordRecoveryController">
   <children>
      <Label text="Password Reset: Step 2">
         <font>
            <Font size="20.0" />
         </font>
         <VBox.margin>
            <Insets left="20.0" top="20.0" />
         </VBox.margin>
      </Label>
      <HBox prefHeight="50.0" prefWidth="521.0">
         <children>
            <Label text="Please answer the following security questions so that we may verify your identity." wrapText="true">
               <font>
                  <Font size="14.0" />
               </font>
            </Label>
         </children>
         <VBox.margin>
            <Insets left="20.0" />
         </VBox.margin>
      </HBox>
      <VBox alignment="CENTER" prefHeight="109.0" prefWidth="541.0" spacing="10.0">
         <children>
            <Label text="Question 1:">
               <font>
                  <Font size="14.0" />
               </font>
            </Label>
            <TextField prefHeight="25.0" prefWidth="345.0">
               <VBox.margin>
                  <Insets left="150.0" right="150.0" />
               </VBox.margin>
            </TextField>
            <Label layoutX="216.0" layoutY="30.0" text="Question 2:">
               <font>
                  <Font size="14.0" />
               </font>
            </Label>
            <TextField layoutX="160.0" layoutY="55.0" prefHeight="25.0" prefWidth="345.0">
               <VBox.margin>
                  <Insets left="150.0" right="150.0" />
               </VBox.margin>
            </TextField>
         </children>
         <VBox.margin>
            <Insets top="10.0" />
         </VBox.margin>
      </VBox>
      <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0" spacing="50.0">
         <children>
            <Button mnemonicParsing="false" text="Continue" />
            <Button layoutX="10.0" layoutY="10.0" mnemonicParsing="false" text="Cancel" />
         </children>
      </HBox>
   </children>
</VBox>
