<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="375.0" prefWidth="541.0" spacing="10.0" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="PasswordRecoveryController">
   <children>
      <Label text="Password Reset: Step 1">
         <font>
            <Font size="20.0" />
         </font>
         <VBox.margin>
            <Insets left="20.0" top="20.0" />
         </VBox.margin>
      </Label>
      <VBox alignment="CENTER" prefHeight="109.0" prefWidth="541.0" spacing="25.0">
         <children>
            <Label fx:id="instructionsLabel" text="Please enter your Account Number">
               <font>
                  <Font size="14.0" />
               </font>
            </Label>
            <TextField fx:id="accountNumberField" prefHeight="25.0" prefWidth="345.0">
               <VBox.margin>
                  <Insets left="150.0" right="150.0" />
               </VBox.margin>
            </TextField>
         </children>
         <VBox.margin>
            <Insets top="30.0" />
         </VBox.margin>
      </VBox>
      <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0" spacing="50.0">
         <children>
            <Button mnemonicParsing="false" onAction="#continueButtonPushed" text="Continue" />
            <Button layoutX="10.0" layoutY="10.0" mnemonicParsing="false" onAction="#cancelButtonPushed" text="Cancel" />
         </children>
      </HBox>
   </children>
</VBox>
